{
  "manifest_version": 3,
  "name": "Salesforce ChangeSet Manager",
  "version": "1.0",
  "description": "Salesforce ChangeSet Manager that can be used for Import/Export ChangeSet content and cloning ChangeSets",
  "permissions": ["activeTab", "scripting", "webNavigation", "tabs"],
  "host_permissions" : ["*://*.force.com/*", "*://*.salesforce.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon_16.png",
      "32": "/images/icon_32.png",
      "48": "/images/icon_48.png",
      "128": "/images/icon_128.png",
      "256": "/images/icon_256.png",
      "512": "/images/icon_512.png"
    }
  },
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_32.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png",
    "256": "/images/icon_256.png",
    "512": "/images/icon_512.png"
  }
}